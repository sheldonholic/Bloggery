<%- include("partials/header"); -%>
    <h1>
        <%= title %>
    </h1>
    <p>
        <%= into%>
    </p>
    <p>
        <% for(var i=0 ; i<posts.length ; i++){ %>
            <h1>
                <%= posts[i].title %>
            </h1>
            <p>
                <%= posts[i].content.slice(0,100)+"..."%>
                    <a href="/posts/<%= posts[i].title %>">Read more</a>
            </p>
            <% }%>
    </p>
    <%- include("partials/footer"); -%>